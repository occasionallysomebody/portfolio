{
  "name": "moonshot-709-portfolio",
  "version": "1.0.0",
  "description": "Minimalist photography portfolio with HEIC support",
  "main": "scripts/optimize.js",
  "scripts": {
    "optimize": "node ./scripts/optimize.js",
    "manifest": "node ./scripts/get-images.js",
    "styles": "tailwindcss -i ./src/input.css -o ./dist/output.css",
    "heic-convert": "python ./scripts/convert_heic.py",
    "include-nav": "node ./scripts/include-nav.js",
    "posts:build": "node ./scripts/md-to-json.js",
    "copy-index": "cp src/index.html dist/index.html",
    "build": "npm run heic-convert && npm run posts:build && npm run include-nav && npm run copy-index && npm run optimize && npm run manifest && npm run styles",
    "postinstall": "pip install -r requirements.txt"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "heic-convert": "^2.1.0",
    "sharp": "^0.33.2",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "lightningcss": "^1.24.1",
    "gray-matter": "^4.0.3"
  }
}